{
    "name": "automation-mock-api",
    "version": "1.0.0",
    "description": "Simple Mock API",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "clean": "rimraf dist",
        "prebuild": "npm run clean"
    },
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.19",
        "typescript": "^5.3.3",
        "ts-node-dev": "^2.0.0",
        "eslint": "^8.56.0",
        "@typescript-eslint/parser": "^7.0.1",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "rimraf": "^5.0.5"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.0.0"
    }
}